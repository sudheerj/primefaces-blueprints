<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.packt.pfblueprints.model.Dealer" table="dealer">
   <id name="id" type="int" column="ID" >
   <generator class="increment"/>
  </id>

  <property name="dealerfirstname">
    <column name="dealerfirstname" />
  </property>
  <property name="dealerlastname">
    <column name="dealerlastname" />
  </property>
  <property name="dealertinnumber">
    <column name="dealertinnumber"/>
  </property>
  <property name="branchname">
    <column name="branchname" />
  </property>
  <property name="dor">
    <column name="dor"/>
  </property>
  <property name="noofadvisors">
    <column name="noofadvisors" />
  </property>
  <property name="pan">
    <column name="pan"/>
  </property>
   <property name="status">
    <column name="status"/>
  </property>
  <property name="address1">
    <column name="address1" />
  </property>
  <property name="address2">
    <column name="address2"/>
  </property>
  <property name="country">
    <column name="country" />
  </property>
  <property name="city">
    <column name="city"/>
  </property>
  <property name="contactnumber">
    <column name="contactnumber" />
  </property>
  <property name="postalcode">
    <column name="postalcode"/>
  </property>
  
 </class>
</hibernate-mapping>